{
  "name": "functions",
  "main": "index.js",
  "engines": {
    "node": "10"
  },
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "copy:example": "cp .env.example ./.env",
    "serve": "firebase serve --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run copy:example && node index.js",
    "format": "npx prettier --write .",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "express-validator": "^6.12.1",
    "firebase-admin": "~9.11.1",
    "firebase-functions": "^3.15.6",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.6.3",
    "nodemailer-smtp-transport": "^2.7.4",
    "stripe": "^9.14.0"
  },
  "devDependencies": {
    "prettier": "^2.4.1"
  },
  "prettier": {
    "printWidth": 120
  },
  "private": true
}
